<section>
  <!-- Content title begins / Début du titre du contenu -->
  <header>
    <h1 id="cn-cont"><%= l(:app_title, :locale => :en) %> / <span lang="fr"><%= l(:app_title, :locale => :fr) %></span></h1>
  </header>
  <!-- Content Title ends / Fin du titre du contenu -->		
  
<!-- clf2-nsi2 theme begins / Début du thème clf2-nsi2 -->
  <!-- Message space begins / Début de l'espace de message -->
  <div id="cn-msg-space" role="banner">
  </div>
  <!-- Message space ends / Fin de l'espace de message -->

  <nav role="navigation">
    <ul id="cn-ef-lang-links">
      <li lang="en"><%= link_to @lang_en[:name], @lang_en[:url], {:title => @lang_en[:title]} %></li>
      <li lang="fr"><%= link_to @lang_fr[:name], @lang_fr[:url], {:title => @lang_fr[:title]} %></li>
    </ul>
  <div id="cn-wmms"><%= image_tag 'wmms.gif', :plugin => 'redmine_clf2', :width => "83", :height=>"20", :alt=>"Symbol of the Government of Canada / Symbole du gouvernement du Canada", :title => "Symbol of the Government of Canada / Symbole du gouvernement du Canada" %></div>
  </nav>
<!-- clf2-nsi2 theme ends / Fin du thème clf2-nsi2 -->
</section>
